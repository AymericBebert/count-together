{
  "name": "count-together",
  "version": "1.0.0",
  "description": "Count Together SPA/PWA",
  "repository": {
    "type": "git",
    "url": "https://github.com/AymericBebert/count-together"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract --input './src/**/!(*.spec).{html,ts}' --output ./src/assets/i18n/{en,fr}.json --clean --sort --format namespaced-json --format-indentation '  ' --string-as-default-value 'MISSING_TRANSLATION'"
  },
  "author": "Aymeric BERNARD",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~13.3.3",
    "@angular/cdk": "~13.3.3",
    "@angular/common": "~13.3.3",
    "@angular/compiler": "~13.3.3",
    "@angular/core": "~13.3.3",
    "@angular/flex-layout": "~13.0.0-beta.38",
    "@angular/forms": "~13.3.3",
    "@angular/material": "~13.3.3",
    "@angular/platform-browser": "~13.3.3",
    "@angular/platform-browser-dynamic": "~13.3.3",
    "@angular/router": "~13.3.3",
    "@angular/service-worker": "~13.3.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "d3": "~7.4.4",
    "rxjs": "~7.5.5",
    "socket.io-client": "~4.4.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@angular/language-service": "~13.3.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/d3": "~7.1.0",
    "@types/dom-mediacapture-record": "^1.0.11",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "5.19.0",
